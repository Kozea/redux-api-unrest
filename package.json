{
  "name": "redux-api-unrest",
  "version": "0.5.1",
  "description": "A redux reducer for unrest.",
  "main": "lib/index.js",
  "repository": "https://github.com/Kozea/redux-api-unrest",
  "author": "Florian Mounier",
  "license": "MIT",
  "scripts": {
    "build": "rm -r lib && babel src --out-dir lib",
    "lint": "eslint src",
    "test": "jest --coverage",
    "coverage": "cat ./coverage/lcov.info | coveralls",
    "test-debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "prepublish": "npm run build"
  },
  "pre-commit": "lint,test",
  "jest": {
    "transform": {
      ".js$": "babel-7-jest"
    }
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.34",
    "@babel/core": "^7.0.0-beta.34",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-beta.34",
    "@babel/preset-env": "^7.0.0-beta.34",
    "babel-7-jest": "^21.3.1",
    "babel-eslint": "^8.0.2",
    "coveralls": "^3.0.0",
    "eslint": "^4.12.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-react": "^7.5.1",
    "import-sort-style-module": "^4.2.0",
    "jest": "^21.2.1",
    "pre-commit": "^1.2.2",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0"
  },
  "files": [
    "lib/"
  ],
  "importSort": {
    ".js, .jsx": {
      "parser": "babylon",
      "style": "module"
    }
  },
  "dependencies": {
    "deep-equal": "^1.0.1",
    "isomorphic-fetch": "^2.2.1",
    "path-to-regexp": "^2.1.0",
    "query-string": "^5.0.1",
    "regenerator-runtime": "^0.11.0"
  }
}
